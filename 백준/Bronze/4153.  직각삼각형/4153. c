#include <stdio.h>
#include <math.h>

int main(){
	int a, b, c;
	scanf("%d %d %d", &a, &b, &c);
	while(a!=0 && b!=0 && c!=0){
		if((int)pow(a, 2)+(int)pow(b, 2)==(int)pow(c, 2))
			printf("right\n");
		else if((int)pow(b, 2)+(int)pow(c, 2)==(int)pow(a, 2))
			printf("right\n");
		else if((int)pow(c, 2)+(int)pow(a, 2)==(int)pow(b, 2))
			printf("right\n");
		else
			printf("wrong\n");
		scanf("%d %d %d", &a, &b, &c);
		
	}
	return 0;
}
